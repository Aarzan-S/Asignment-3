    <children>
        <Button layoutX="32.0" layoutY="24.0" mnemonicParsing="false" onAction="#navigateBack" text="Back"/>
        <TextField fx:id="searchBox" layoutX="406.0" layoutY="56.0" promptText="Type to search"/>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="170.0" layoutY="10.0" prefHeight="25.0"
               prefWidth="256.0" text="Recruit Details">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label fx:id="infoLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="180.0" layoutY="58.0"
               prefHeight="19.0" prefWidth="235.0" textFill="#72d46d"/>
        <TableView fx:id="tableView" layoutX="13.0" layoutY="89.0" prefHeight="269.0" prefWidth="582.0"/>
    </children>
</AnchorPane>
